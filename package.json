{
  "name": "infosec-agent",
  "description": "InfoSec Agent by Fontys ICT and Guardian360",
  "main": "./out/main/index.js",
  "author": "Fontys ICT",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://fontysict.nl",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "buildlinux": "tsc && vite build && electron-builder -l",
    "buildwin": "tsc && vite build && electron-builder -w",
    "buildlinuxwin": "tsc && vite build && electron-builder -wl",
    "buildwinlinux": "tsc && vite build && electron-builder -wl",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@mantine/charts": "^7.15.2",
    "@mantine/core": "^7.14.3",
    "@mantine/hooks": "^7.14.3",
    "@radix-ui/react-form": "^0.1.0",
    "@radix-ui/react-icons": "^1.3.1",
    "@radix-ui/react-menubar": "^1.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.1",
    "@radix-ui/themes": "^3.1.4",
    "electron-updater": "^6.1.7",
    "recharts": "2",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@tanstack/eslint-plugin-query": "^5.59.7",
    "@tanstack/react-query": "^5.59.16",
    "@tanstack/react-query-devtools": "^5.59.16",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.3.1",
    "axios": "^1.7.7",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.49",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.2",
    "usehooks-ts": "^3.1.0",
    "vite": "^5.3.1",
    "vite-plugin-electron": "^0.28.6",
    "vite-tsconfig-paths": "^5.0.1"
  }
}
